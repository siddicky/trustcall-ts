{
  "name": "trustcalljs",
  "version": "0.2.9",
  "description": "Utilities for validated tool calling and extraction with retries using LLMs - TypeScript port of trustcall",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "test:unit": "vitest --run --exclude 'tests/integration/**'",
    "test:integration": "vitest --run tests/integration",
    "lint": "eslint src",
    "format": "prettier --write 'src/**/*.ts' 'tests/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts' 'tests/**/*.ts'",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "langchain",
    "langgraph",
    "llm",
    "structured-output",
    "extraction",
    "validation",
    "tool-calling"
  ],
  "author": "Abdullah Siddique <abdullah.siddique94@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/siddicky/trustcall-ts.git"
  },
  "dependencies": {
    "@langchain/core": "^0.3.0",
    "@langchain/langgraph": "^0.2.0",
    "uuid": "^9.0.0",
    "zod": "^3.22.0",
    "zod-to-json-schema": "^3.22.0"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.52.0",
    "@langchain/anthropic": "^0.3.0",
    "@langchain/openai": "^0.3.0",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "dotenv": "^17.2.3",
    "eslint": "^8.55.0",
    "prettier": "^3.8.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "@langchain/anthropic": ">=0.3.0",
    "@langchain/openai": ">=0.3.0"
  },
  "peerDependenciesMeta": {
    "@langchain/openai": {
      "optional": true
    },
    "@langchain/anthropic": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18"
  }
}